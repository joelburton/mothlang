var secretNum = (randomNum (1.0, 100.0));
var guess = (- 1.0);
print "I guessed a number. Can you guess it?";
fun game() {
while ((secretNum != guess)) {
(guess = (stringToNum ((input ("? ")))));
if ((guess == secretNum)) {
print "you win!";
return nil;
}
if ((guess < secretNum)) {
print "too low, numskull!";
} else {
print "whoa there, buddy --- too high!";
}

}
}
(game ());
